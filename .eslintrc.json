{
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "es2017": true,
    "node": true
  },
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "eslint-config-archereducation"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "node/no-missing-import": "off",
    "import/no-unresolved": "off"
  }
}
